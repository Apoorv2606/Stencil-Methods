    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@se/fonts": "^1.2.3",
    "@se/icons": "^4.9.6",
    "@se/web-ui": "^3.26.11",
    "@stencil/core": "^3.0.0",
    "@stencil/sass": "^3.0.4",
    "@stencil/store": "^2.0.8"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "puppeteer": "^19.5.2"
  },
  "license": "MIT"
}
